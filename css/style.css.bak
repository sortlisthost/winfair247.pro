.bl-box {
  height: 32px;
  width: 72px;
  border-radius: 0;
  padding: 5px 0;
  text-align: center;
  margin-right: 4px;
  float: left;
  cursor: pointer;
  position: relative;
  -webkit-transition: color 0.5s;
  transition: color 0.5s;
}

.bl-box.odds-down {
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
}

.bl-box.odds-up {
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
}

.bl-box:before {
  content: "";
  position: absolute;
  z-index: -1;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 4px;
}

.bl-box.suspended:before {
  z-index: 1;
}

.bl-box.odds-down:before {
  background: #ff000080;
  transform: scale(0, 0);
  animation: oddsdown 0.5s ease-out;
}

.bl-box.odds-up:before {
  background: #13c41380;
  transform: scale(0, 0);
  animation: oddsup 0.5s ease-out;
}

@keyframes oddsdown {
  from {
    transform: scale(0);
  }

  to {
    transform: scale(1);
  }
}

@keyframes oddsup {
  from {
    transform: scale(0);
  }

  to {
    transform: scale(1);
  }
}

